<%= form_for @post do |f| %>
  <div class="form_control">
      <%= f.label :title %>
      <%= f.text_field :title %>
  </div>

  <div class="form_control">
    <%= f.label :body %>
    <%= f.text_field :body %>
  </div>

  <div class="form_control">
    <%= f.submit 'Зберегти' %>
  </div>
<% end %>